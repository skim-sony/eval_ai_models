{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "version": "1.0.1",
  "type": "object",
  "properties": {
    "parking_config": {
      "type": "object",
      "properties": {
        
        "parking_zone": {
          "type": "object",
          "properties": {
            "type": {
              "type": "string",
              "enum": ["Polygon"]
            },
            "coordinates": {
              "type": "array",
              "items": {
                "type": "array",
                "items": {
                  "type": "array",
                  "items": {
                    "type": "number"
                    },
                    "minItems": 2,
                    "maxItems": 2
                  }
                },
                "minItems": 1
              }
            },
            "required": ["type", "coordinates"]
          }
        }
      }
    }
}