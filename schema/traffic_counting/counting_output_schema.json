{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "aitrios:app:output:counting",
  "version": "1.0.1",
  "type": "object",
  "properties": {
    "boundary_id": {
      "type": "string",
      "description": "Boundary ID for traffic counting"
    },
    "vehicleType": {
      "type": "string",
      "enum": ["bicycle", "cargo_bicycle", "car", "scooter", "moped", "motorcycle", "truck", "van", "freight", "other", "unspecified", "pedestrian"],
      "description": "Vehicle type for counting"
    },
    "vehicleCount": {
      "type": "integer",
      "description": "Total number of vehicles counted"
    }
  },
  "additionalProperties": false
}